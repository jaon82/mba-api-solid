  "devDependencies": {
    "@eslint/js": "9.16.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "22.10.1",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/ui": "2.1.8",
    "eslint": "9.16.0",
    "globals": "15.13.0",
    "prisma": "6.0.1",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@fastify/jwt": "9.0.2",
    "@prisma/client": "6.0.1",
